#Properties File
server.type=<%= @server_type %>

#Data Loader Properties
dataloader.localdir=<%= @dataloader_localdir %>
dataloader.workingdir=<%= @dataloader_workingdir %>
dataloader.threadcount=<%= @dataloader_threadcount %>
dataloader.sleeptime=<%= @dataloader_sleeptime %>
dataloader.monitor_dir=<%= @dataloader_monitor_dir %>
dataloader.threadclass=com.MyRepMoney.processor.MyRepMoneyDataLoadProcessorTask
dataloader.messagevisibilitytask.sleeptime=60

#JCS Properties
jcs.sleeptime=<%= @jcs_sleeptime %>
jcs.monitor_dir=<%= @jcs_monitor_dir %>
jcs.threadcount=1
jcs.threadclass=com.MyRepMoney.processor.MyRepMoneyControlProcessorTask


SourceDataSet.delimiter=^

database.userid=<%= @db_username %>
database.password=<%= @db_password %>
database.jdbcdriver=
database.url=jdbc:mysql://<%= @db_host %>:<%= @db_port %>/<%= @db_name %>

AWS_SQS_Queue=<%= @aws_sqs_queue %>
AWS_Region=<%= @aws_region %>
AWS_SQS_Message_Delay=1
AWS_SQS_Url=<%= @aws_sqs_url %>
AWS_SQS_messagevisibility=65